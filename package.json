{
  "name": "eatery",
  "version": "1.0.0",
  "description": "Fullstack recipe web app where users can search and store their favorite recipies",
  "main": "server/server.js",
  "scripts": {
    "test": "jest",
    "start": "node server/server.js",
    "server": "nodemon server/server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently --raw --kill-others \"npm run server\" \"npm run client\""
  },
  "author": "Brandon Carmody",
  "license": "MIT",
  "devDependencies": {
    "jest": "^23.6.0",
    "nodemon": "^1.18.9",
    "supertest": "^3.4.1"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.11",
    "mongoose": "^5.4.4",
    "mongoose-unique-validator": "^2.0.2",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "validator": "^10.11.0"
  },
  "events": {
    "restart": "fuser -k 5000/tcp ; fuser -k 3000/tcp"
  }
}
